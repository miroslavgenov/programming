<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <corners
        android:radius="50dp"
        />
    <gradient
        android:angle="315"

        android:centerY="35%"

        android:centerColor="#D41872"
        android:startColor="#A445B2"
        android:endColor="#FFC0FF7D"
        android:type="linear"
        />
    <padding
        android:left="0dp"
        android:top="0dp"
        android:right="0dp"
        android:bottom="0dp"
        />

    <stroke
        android:width="1dp"

        android:dashWidth="10dp"
        android:color="#9FCCFF96"
        />
    <size
        android:width="100dp"
        android:height="100dp"
        />
</shape>













