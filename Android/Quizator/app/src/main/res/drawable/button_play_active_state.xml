<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle" >
    <corners
        android:radius="100dp"
        />
    <gradient
        android:angle="135"

        android:centerColor="#D41872"
        android:startColor="#FF0066"
        android:endColor="#A445B2"
        android:type="linear"
        />
    <padding
        android:left="0dp"
        android:top="0dp"
        android:right="0dp"
        android:bottom="0dp"
        />
    <size
        android:width="100dp"
        android:height="100dp"
        />
    <stroke
        android:width="1dp"
        android:dashGap="20dp"
        android:dashWidth="10dp"
        android:color="#E0CCFF96"
        />


</shape>



















































