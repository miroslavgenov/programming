package abstractexample;

abstract class Animal {
	private String aType;
	private Integer age;
	
	public Animal(String aType,Integer age) {
		this.aType=aType;
		this.age=age;
	}
	
	public abstract void showAnimalType();
	
	public String getAnimalType() {return this.aType;}
	
	public void showAge() {
		System.out.println();
	}
}

class Dog extends Animal{
	
	Dog(Integer age){
		super("dog",age);
	}
	
	@Override
	public void showAnimalType() {
		System.out.println(super.getAnimalType());
	}
}

public class UseAbstract{
	
}

























































