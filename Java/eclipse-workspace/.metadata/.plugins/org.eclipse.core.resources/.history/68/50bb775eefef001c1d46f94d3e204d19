package testinterafece;

interface Speakable{
	public abstract void speak();
	public abstract void show(String n);}

abstract class Animal implements Speakable{
	private String name;
	private Integer age;
	
	public Animal(String name, Integer age) {
		super();
		this.name = name;
		this.age = age;
	}

	@Override
	public void show(String n) {
		
		
	}
	
	
	
	
}




